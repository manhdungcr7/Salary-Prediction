    {
      "cell_type": "code",
      "source": [
        "model = LinearRegression(fit_intercept=False)\n",
        "model.fit(X,Y)\n",
        "print(f\"w: {np.array([model.intercept_, *model.coef_[0]])}\")\n",
        "Y_pred = model.predict(X)\n",
        "loss = mse(Y, Y_pred)\n",
        "print(f\"MSE: {loss}\")"
