    {
      "cell_type": "code",
      "source": [
        "ols_data=data[[\"Age\",\"Year_Exp\",\"Salary\"]]\n",
        "X=ols_data[[\"Age\",\"Year_Exp\"]].values\n",
        "Y=ols_data[\"Salary\"].values.reshape(-1,1)"
